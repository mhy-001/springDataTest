<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.test.repository.StudentRepository">
    <select id="findAll" resultType="Student">
        select*from student
    </select>
    <select id="findById" resultType="Student" parameterType="Integer">
        select * from student where id = #{id}
    </select>
    <insert id="saveStudent" parameterType="Student">
        insert into student values (#{id},#{name},#{age},#{sex})
    </insert>
    <delete id="deleteStuById" parameterType="Integer">
        delete from student where id = #{id}
    </delete>
</mapper>